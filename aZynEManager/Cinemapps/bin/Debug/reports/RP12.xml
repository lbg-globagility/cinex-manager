  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select a.code, @patron:= a.name as PATRON,  @cinema:= c.name 
CINEMA, b.price PRICE, @c:= (select count(f.cinema_seat_id) 
from patrons a inner join cinema_patron b on a.id=b.patron_id 
inner join  cinema c on b.cinema_id = c.id 
inner join movies_schedule d on d.cinema_id = c.id 
inner join movies_schedule_list e on
e.movies_schedule_id = d.id inner join 
movies_schedule_list_reserved_seat f on 
f.movies_schedule_list_id= e.id where d.movie_date='2014/10/31'and a.name=@patron 
and c.name = @cinema) as QTY, (@c*b.price) as 
`TOTALSALES`, d.movie_date , h.start_date, h.title, i.userid,
 i.fname, i.lname, i.mname, j.system_value
from patrons a inner join cinema_patron 
b on a.id=b.patron_id inner join  cinema c on 
b.cinema_id = c.id inner join movies_schedule d 
on d.cinema_id = c.id inner join movies_schedule_list 
e on e.movies_schedule_id = d.id inner join 
movies_schedule_list_reserved_seat f on 
f.movies_schedule_list_id= e.id inner join
ticket g on e.id= g.movies_schedule_list_id 
inner join movies h on h.id= d.movie_id 
inner join users i on i.id= g.user_id inner join config_table j
where d.movie_date='2014/10/31' and j.system_code='001'  and f.status=1 
group by a.name, c.name order by c.name;</CommandText>
