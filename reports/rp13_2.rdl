  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select * from 
(select f.code patroncode, f.name patronname, i.name cinemaname, b.price price, 
count(b.cinema_seat_id) qty, sum(b.price) totalsales, h.movie_date, 
d.userid, concat_ws(' ', d.fname, d.mname, d.lname) usernm, 
j.system_value, k.name reportname, l.title
from movies_schedule_list_reserved_seat b, ticket c,
users d, movies_schedule_list_patron e, patrons f, movies_schedule_list g,
movies_schedule h, cinema i, config_table j, report k, movies l
where b.movies_schedule_list_id in
(select a.id from movies_schedule_list a
where a.start_time &gt; '2014-11-23'
and a.start_time &lt; '2014-11-24'
and a.status = 1)
and b.ticket_id = c.id
and b.movies_schedule_list_id = g.id
and c.user_id = d.id
and d.system_code = 2
and d.status = 1
and b.status = 1
and b.patron_id = e.id
and e.patron_id = f.id
and g.movies_schedule_id = h.id
and h.cinema_id = i.id
and h.movie_id = l.id
and j.system_code = '001'
and k.id = 13
group by h.cinema_id, h.movie_id, c.user_id, e.patron_id
order by h.cinema_id, c.user_id, f.code asc) tbl
order by cinemaname, usernm, patroncode asc</CommandText>
      </Query>
