  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select cinemacode, cinemaname, sum(seattaken), sum(seattotal), sum(totalprice), ((sum(seattaken)/sum(seattotal)) * 100) util,
system_value, reportname from (select f.id cinemacode, f.name cinemaname, 
count(b.cinema_seat_id) seattaken, f.capacity * count(distinct(c.id)) seattotal, 
sum(b.price) totalprice, j.system_value, k.name reportname 
from movies_schedule_list_reserved_seat b, movies_schedule_list c, movies_schedule d, movies e, 
cinema f, config_table j, report k 
where b.movies_schedule_list_id in 
(select a.id from movies_schedule_list a where a.start_time &gt; '2014/11/30' 
and a.start_time &lt; '2014/12/01' and a.status = 1) 
and b.movies_schedule_list_id = c.id and c.movies_schedule_id = d.id 
and d.movie_id = e.id and d.cinema_id = f.id and b.status = 1 
and j.system_code = '001' and k.id = 17 
group by d.cinema_id,e.code 
order by d.cinema_id,e.title asc) tbl1
group by cinemacode 
order by cinemacode</CommandText>
      </Query>
      <Fields>
        <Field Name="cinemacode">
          <DataField>cinemacode</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="cinemaname">
          <DataField>cinemaname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sum_seattaken_">
          <DataField>sum(seattaken)</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sum_seattotal_">
          <DataField>sum(seattotal)</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="sum_totalprice_">
          <DataField>sum(totalprice)</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="util">
          <DataField>util</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="system_value">
          <DataField>system_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="reportname">
          <DataField>reportname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
