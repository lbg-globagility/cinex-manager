<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2005/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <Description>Ordinance by Movie per Day</Description>
  <Author>rogeliobetito</Author>
  <PageHeight>11in</PageHeight>
  <PageWidth>8.5in</PageWidth>
  <Width>7.5in</Width>
  <TopMargin>.25in</TopMargin>
  <LeftMargin>.25in</LeftMargin>
  <RightMargin>.25in</RightMargin>
  <BottomMargin>.25in</BottomMargin>
  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT 
    h.system_value,
    g.name,
    d.title,
    d.code,
    c.movie_date,
    i.details surcharge, 
    a.surcharge_price price,
    count(i.amount_val) quantity,
    SUM(a.surcharge_price) amount
FROM
    movies_schedule_list_reserved_seat a,
    movies_schedule_list b,
    movies_schedule c,
    movies d,
    patrons e,
    movies_schedule_list_patron f,
    report g,
    config_table h,
    surcharge_tbl i,
    patrons_surcharge j
WHERE
    a.movies_schedule_list_id = b.id
        AND b.movies_schedule_id = c.id
        AND c.movie_id = d.id
        AND a.patron_id = f.id
        AND f.patron_id = e.id
        AND e.id = j.patron_id
        AND j.surcharge_id = i.id
        AND i.in_pesovalue = 1
        AND a.status = 1
        AND h.system_code = '001'
        AND g.id = 20
        AND c.movie_date &gt;= '2022/04/27'
        AND c.movie_date &lt; '2022/04/28'
GROUP BY d.title , c.movie_date, a.surcharge_price
ORDER BY d.title , c.movie_date ASC</CommandText>
      </Query>
      <Fields>
