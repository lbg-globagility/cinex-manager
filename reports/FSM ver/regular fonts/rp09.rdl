  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select e.id cinema_id, e.name cinema_name,  d.code patron_code, d.name patron_name, a.price, 
COUNT(a.cinema_seat_id) qty, SUM(a.price) sales, g.system_value report_header, h.name report_title 
from movies_schedule_list_reserved_seat a, movies_schedule_list b, 
movies_schedule c, patrons d, cinema e, movies_schedule_list_patron f, 
config_table g, report h 
where a.movies_schedule_list_id in (select distinct(bb.id) id from movies_schedule_list bb 
where bb.start_time between '2016-1-1' and '2016-1-2' and bb.status = 1) 
AND a.movies_schedule_list_id = b.id  
AND b.movies_schedule_id = c.id 
AND a.patron_id = f.id
AND f.patron_id = d.id 
AND c.cinema_id = e.id 
and g.system_code = '001' 
and h.id = 9 
GROUP BY c.cinema_id, f.patron_id, a.price 
ORDER BY e.in_order, d.name, a.price</CommandText>
      </Query>
      <Fields>
        <Field Name="cinema_id">
          <DataField>cinema_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="cinema_name">
          <DataField>cinema_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="patron_code">
