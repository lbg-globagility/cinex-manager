  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT table1.voidamount, table2.totalcnt, table2.totalgross, c.system_value, d.name, table2.movie_date  
from (SELECT IFNULL(SUM(a.price), 0) AS voidamount
FROM movies_schedule_list_reserved_seat a, movies_schedule_list b, movies_schedule c, ticket d
WHERE a.movies_schedule_list_id = b.id AND b.movies_schedule_id = c.id 
AND a.ticket_id = d.id
AND c.movie_date = '2016-01-01' AND a.status = 0)table1,
(SELECT IFNULL(COUNT(a.id), 0) AS totalcnt, IFNULL(SUM(a.price), 0) AS totalgross, c.movie_date 
FROM movies_schedule_list_reserved_seat a, movies_schedule_list b, movies_schedule c, ticket e 
WHERE a.movies_schedule_list_id = b.id AND b.movies_schedule_id = c.id  AND a.ticket_id = e.id
AND c.movie_date = '2016-01-01' AND a.status = 1) table2, config_table c, report d 
WHERE c.system_code = '001'
and d.id = 19</CommandText>
      </Query>
      <Fields>
        <Field Name="voidamount">
          <DataField>voidamount</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="totalcnt">
          <DataField>totalcnt</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="totalgross">
