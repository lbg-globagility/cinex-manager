  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select d.code as movie_code, d.title, count(a.cinema_seat_id) as qty,
sum(a.price) as Sales, c.movie_date, e.name dist_name, 
e.code as dist_code, g.system_value, h.name report_name
from movies_schedule_list_reserved_seat a, movies_schedule_list b, 
movies_schedule c, movies d, distributor e, config_table g, report h
where a.movies_schedule_list_id = b.id
and b.movies_schedule_id = c.id
and c.movie_id = d.id
and d.dist_id = e.id
and c.movie_date = '2016-1-1'
 and e.code = 'STAR'
and a.status = 1
and b.status = 1
and g.system_code = '001' 
and h.id = 5
group by e.code, d.code
order by d.title asc</CommandText>
      </Query>
      <Fields>
        <Field Name="movie_code">
          <DataField>movie_code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="title">
          <DataField>title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="qty">
          <DataField>qty</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="Sales">
          <DataField>Sales</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="movie_date">
          <DataField>movie_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="dist_name">
