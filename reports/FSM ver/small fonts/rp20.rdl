  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>select h.system_value, g.name, d.title, d.code, c.movie_date, sum(i.amount_val) sur_amt, sum(a.price - a.base_price) amount
from movies_schedule_list_reserved_seat a, movies_schedule_list b, 
movies_schedule c, movies d, patrons e, movies_schedule_list_patron f, report g, config_table h,
surcharge_tbl i, patrons_surcharge j
where a.movies_schedule_list_id= b.id 
and b.movies_schedule_id = c.id 
and c.movie_id = d.id 
and a.patron_id = f.id
and f.patron_id = e.id
and e.id = j.patron_id
and j.surcharge_id = i.id
and i.in_pesovalue = 1
and a.status = 1
and h.system_code='001' 
and g.id = 20
and c.movie_date &gt;= '2016/1/1'
and c.movie_date &lt; '2016/1/3' 
group by d.title,c.movie_date
order by d.title,c.movie_date asc</CommandText>
      </Query>
      <Fields>
        <Field Name="system_value">
          <DataField>system_value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="title">
          <DataField>title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="code">
          <DataField>code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="movie_date">
          <DataField>movie_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="sur_amt">
          <DataField>sur_amt</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="amount">
          <DataField>amount</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
